{
  "manifest_version": 2,
  "name": "Auto Archive 2",
  "description": "Auto Archive extension based on Awesome Auto Archive",
  "version": "0.1",
  "author": "Bill Stumbo",
  "applications": {
    "gecko": {
      "id": "autoArchive2@github.com",
      "strict_min_version": "66.0"
    }
  },
  "browser_action": {
    "default_popup": "content/popup.html",
    "default_title": "Auto Archive 2",
    "default_icon": "images/icon.png"
  },
  "options_ui" : {
    "page": "content/dialog2.xhtml",
    "open_in_tab" : true,
    "browser_style": true
  },
  "permissions": [

  ],
  "icons" : {
    "64": "images/icon.png",
    "32": "images/icon.png",
    "16": "images/icon.png"
  }
}