{
  "name": "Auto Archive 2",
  "description": "Auto Archive extension based on Awesome Auto Archive",
  "version": "0.1",
  "author": "Bill Stumbo",
  "homepage_url" : "https://github.com/stumbo/autoarchive2",
  "applications": {
    "gecko": {
      "id": "autoArchive2@github.com",
      "strict_min_version": "78.0"
    }
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Auto Archive 2",
    "default_icon": "images/icon.png"
  },

  "background" : {
    "scripts" : [
      "background.js"
    ]
  },
  "permissions": [
    "menus",
    "messagesRead",
    "accountsRead"
  ],
  "icons" : {
    "64": "images/icon.png",
    "32": "images/icon.png",
    "16": "images/icon.png"
  },

  "manifest_version": 2
}